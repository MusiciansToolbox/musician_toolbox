<header>
  <h1>Musicians Toolbox</h1>
  
  <div class="nav-bar">
    <ul>
      <% if current_user %>
      <li>
        <%= link_to "Home", root_path %>
      </li>
      <li>
        <%= link_to "new file", new_file_path %>
      </li>
      <li>
        <%= link_to "Logout", logout_path, method: :delete %>
      </li>
      <% else %><!--stuff to show up if not logged in, in top right header -->
      <li>
        <%= link_to "Login", login_path %>
      </li>
      <% end %>
    </ul>
  </div>
</header>
