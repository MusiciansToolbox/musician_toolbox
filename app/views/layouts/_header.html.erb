<header>
  <h1><%= link_to "Jam Connect", root_path %></h1>

  <div class="header-profile">
    <% if session[:user_id] %>
      <%= link_to root_path do %>
        <%= image_tag @user.profile_picture.url, class: "profile-pic" %>
      <% end %>


    <% else %>
      <!--stuff to show up if not logged in, in top right header -->
    <% end %>
  </div>
</header>
