<!DOCTYPE html>
<html>
<head>
  <title>MusicianToolbox</title>
  <%= stylesheet_link_tag    'application', media: 'all' %>
  <%= javascript_include_tag 'application' %>
  <%= csrf_meta_tags %>
</head>
<body>
  <p class="notice"><%= notice %></p>
  <p class="alert"><%= alert %></p>

  <% if current_user %>
    <%= render 'layouts/header' %>
  <% end %>

  <div class="container">
    <%= yield %>
  </div>

</body>
</html>
